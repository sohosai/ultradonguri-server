{
    "swagger": "2.0",
    "info": {
        "description": "Sohosai 2025 project telop sending API",
        "title": "Ultradonguri API",
        "contact": {},
        "version": "0.1"
    },
    "basePath": "/",
    "paths": {
        "/conversion/cm-mode": {
            "post": {
                "description": "endpoint for conversion to cm-mode",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "conversion"
                ],
                "summary": "conversion cm-mode",
                "parameters": [
                    {
                        "description": "post CM request",
                        "name": "CMStateStart",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/requests.CMStateRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/responses.SuccessResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/responses.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/conversion/start": {
            "post": {
                "description": "endpoint for start conversion",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "conversion"
                ],
                "summary": "conversion start",
                "parameters": [
                    {
                        "description": "post conversion request",
                        "name": "conversionStart",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/requests.ConversionRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/responses.SuccessResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/responses.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/force_mute": {
            "post": {
                "description": "endpoint for force mute and prevent changes",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "force_mute"
                ],
                "summary": "force mute",
                "parameters": [
                    {
                        "description": "select mute state",
                        "name": "isMuted",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/requests.MuteStateRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/responses.SuccessResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/responses.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/health": {
            "get": {
                "description": "endpoint for health check",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "health"
                ],
                "summary": "health check",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/responses.SuccessResponse"
                        }
                    }
                }
            }
        },
        "/performance/music": {
            "post": {
                "description": "endpoint for start performance music",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "performance"
                ],
                "summary": "start performance music",
                "parameters": [
                    {
                        "description": "post performance music",
                        "name": "performanceMusic",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/requests.MusicRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/responses.SuccessResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/responses.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/performance/start": {
            "post": {
                "description": "endpoint for start performance",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "performance"
                ],
                "summary": "start performance",
                "parameters": [
                    {
                        "description": "post performance request",
                        "name": "performanceStart",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/requests.PerformanceRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/responses.SuccessResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/responses.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/performances": {
            "get": {
                "description": "endpoint for get performances data",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "performances"
                ],
                "summary": "get performances data",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/responses.PerformancesResponse"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/responses.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "requests.CMStateRequest": {
            "type": "object",
            "properties": {
                "is_cm_mode": {
                    "type": "boolean"
                }
            }
        },
        "requests.ConversionRequest": {
            "type": "object",
            "properties": {
                "next_performances": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/requests.NextPerformanceRequest"
                    }
                }
            }
        },
        "requests.MusicRequest": {
            "type": "object",
            "properties": {
                "artist": {
                    "type": "string"
                },
                "should_be_muted": {
                    "type": "boolean"
                },
                "title": {
                    "type": "string"
                }
            }
        },
        "requests.MuteStateRequest": {
            "type": "object",
            "properties": {
                "is_muted": {
                    "type": "boolean"
                }
            }
        },
        "requests.NextPerformanceRequest": {
            "type": "object",
            "properties": {
                "description": {
                    "type": "string"
                },
                "performer": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                }
            }
        },
        "requests.PerformanceRequest": {
            "type": "object",
            "properties": {
                "performer": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                }
            }
        },
        "responses.ErrorResponse": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "message": {
                    "type": "string"
                }
            }
        },
        "responses.MusicResponse": {
            "type": "object",
            "properties": {
                "artist": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "intro": {
                    "type": "string"
                },
                "should_be_muted": {
                    "type": "boolean"
                },
                "title": {
                    "type": "string"
                }
            }
        },
        "responses.PerformancesResponse": {
            "type": "object",
            "properties": {
                "description": {
                    "type": "string"
                },
                "ends_at": {
                    "type": "integer"
                },
                "id": {
                    "type": "string"
                },
                "musics": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/responses.MusicResponse"
                    }
                },
                "performer": {
                    "type": "string"
                },
                "starts_at": {
                    "type": "integer"
                },
                "title": {
                    "type": "string"
                }
            }
        },
        "responses.SuccessResponse": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string"
                }
            }
        }
    }
}